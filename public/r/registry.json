{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "base",
  "homepage": "https://github.com/aayushbtw/registry",
  "items": [
    {
      "name": "globals",
      "type": "registry:style",
      "title": "Globals",
      "description": "Default Tailwind config for all projects.",
      "files": [
        {
          "path": "registry/default/styles/globals.css",
          "type": "registry:style",
          "target": "base/globals.css"
        }
      ],
      "dependencies": ["tailwindcss@^4.1.18", "@tailwindcss/typography"]
    },
    {
      "name": "card",
      "type": "registry:ui",
      "title": "Card",
      "description": "Compound card component.",
      "files": [
        {
          "path": "registry/default/ui/card.tsx",
          "type": "registry:component",
          "target": "base/ui/card.tsx"
        }
      ]
    },
    {
      "name": "animated-link-text",
      "type": "registry:ui",
      "title": "Animated Link Text",
      "description": "Link text with animated brackets.",
      "files": [
        {
          "path": "registry/default/ui/animated-link-text.tsx",
          "type": "registry:component",
          "target": "base/ui/animated-link-text.tsx"
        }
      ]
    },
    {
      "name": "navigation-menu",
      "type": "registry:ui",
      "title": "Navigation Menu",
      "description": "Navigation menu with active states.",
      "files": [
        {
          "path": "registry/default/ui/navigation-menu.tsx",
          "type": "registry:component",
          "target": "base/ui/navigation-menu.tsx"
        }
      ]
    },
    {
      "name": "navbar",
      "type": "registry:block",
      "title": "Navbar",
      "description": "Navbar component with active route detection.",
      "files": [
        {
          "path": "registry/default/blocks/navbar.tsx",
          "type": "registry:block",
          "target": "base/navbar.tsx"
        }
      ],
      "registryDependencies": ["base/navigation-menu"]
    }
  ]
}
